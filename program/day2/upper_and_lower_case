class upper_lower{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        // .trim() removes extra spaces before first charecter
        char ch=sc.next().trim().charAt(0);
        if(ch>='a' && ch<='z'){
            System.out.println("lower");
        }
        else if(ch>='A' && ch<='Z')
        System.out.println("upper");
        }
    }
}